BEGIN:VCALENDAR
METHOD:PUBLISH
VERSION:2.0
PRODID:-//newthinking communications GmbH 
X-WR-CALNAME:re:publica 2013 Schedule
X-APPLE-CALENDAR-COLOR:#00C6FF
X-WR-TIMEZONE:Europe/Berlin
X-LIC-LOCATION:Europe/Berlin
CALSCALE:GREGORIAN
BEGIN:VTIMEZONE
TZID:Europe/Berlin
BEGIN:DAYLIGHT
TZOFFSETFROM:+0100
TZOFFSETTO:+0200
TZNAME:CEST
DTSTART:19700329T020000
RRULE:FREQ=YEARLY;BYDAY=-1SU;BYMONTH=3
END:DAYLIGHT
BEGIN:STANDARD
TZOFFSETFROM:+0200
TZOFFSETTO:+0100
TZNAME:CET
DTSTART:19701025T030000
RRULE:FREQ=YEARLY;BYDAY=-1SU;BYMONTH=10
